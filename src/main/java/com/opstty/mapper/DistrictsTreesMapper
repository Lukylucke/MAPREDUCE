  
package com.opstty.mapper;

import org.apache.hadoop.io.NullWritable;
import org.apache.hadoop.io.Object;
import org.apache.hadoop.io.Text;
import org.apache.hadoop.mapreduce.Mapper;

import java.io.IOException;

public class TokenizerMapper extends Mapper<Object, Text, Text, NullWritable> {

    public void map(Object key, Text value, Context context) throws IOException, InterruptedException {
        if (!value.toString().contains("Districts")){
            Text DistrictsTrees = new Text(value.toString().split().split()(":")[1])
            context.write(DistrictsTrees, NullWritable.get());
        }
    }
}
